# Database Configuration
POSTGRES_USER=scribbl_user
POSTGRES_PASSWORD=your_secure_postgres_password
POSTGRES_DB=scribbl_db
POSTGRES_HOST=postgres

# Application Security
SECRET_KEY_BASE=change-this-to-a-strong-random-secret-at-least-64-chars-long-for-production

# Domain Configuration
DOMAIN_NAME=scribbl.

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
AUTH_REDIS_DB=0
SCRIBBL_REDIS_DB=1

# Auth Service Configuration
TWO_FACTOR_API_KEY=your_2factor_api_key_here
OTP_TEMPLATE_NAME=your_registered_template_name
RATE_LIMIT_PER_MINUTE=5

# CORS Configuration - comma-separated list of allowed origins
CORS_ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com